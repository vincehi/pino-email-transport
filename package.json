{
	"name": "pino-email-transport",
	"version": "0.1.5",
	"description": "Pino transport to send logs via email (SMTP)",
	"keywords": [
		"email",
		"smtp",
		"logger",
		"logging",
		"node",
		"pino",
		"transport"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vincehi/pino-email-transport.git",
		"directory": "/"
	},
	"author": "vincehi",
	"homepage": "https://github.com/vincehi/pino-email-transport",
	"license": "ISC",
	"module": "dist/es6/index.js",
	"types": "dist/es6/index.d.ts",
	"main": "dist/es6/index.js",
	"exports": {
		".": {
			"types": "./dist/es6/index.d.ts",
			"import": "./dist/es6/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/es6/index.js"
		}
	},
	"bugs": {
		"url": "https://github.com/vincehi/pino-email-transport/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"pino": "^7.0.0 || ^8.0.0 || ^9.0.0"
	},
	"dependencies": {
		"nodemailer": "^6.9.16",
		"pino-abstract-transport": "^1.0.0",
		"pino-pretty": "^11.3.0"
	},
	"scripts": {
		"build:cjs": "tsc",
		"build:es6": "tsc -p tsconfig.es6.json",
		"build": "npm run build:cjs && npm run build:es6",
		"prepublishOnly": "npm run build",
		"test": "echo \"Error: run tests from root\" && exit 1"
	},
	"files": ["dist"],
	"devDependencies": {
		"@types/events": "^3.0.0",
		"@types/node": "^20.16.11",
		"@types/nodemailer": "^6.4.15",
		"typescript": "^5.6.2"
	},
	"private": false
}
