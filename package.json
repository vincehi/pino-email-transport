{
	"name": "pino-email-transport",
	"version": "0.2.1",
	"description": "Pino transport to send logs via email (SMTP)",
	"keywords": [
		"email",
		"smtp",
		"logger",
		"logging",
		"node",
		"pino",
		"transport"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vincehi/pino-email-transport.git",
		"directory": "/"
	},
	"author": "vincehi",
	"homepage": "https://github.com/vincehi/pino-email-transport",
	"license": "ISC",
	"module": "dist/es6/index.js",
	"types": "dist/es6/index.d.ts",
	"main": "dist/es6/index.js",
	"exports": {
		".": {
			"types": "./dist/es6/index.d.ts",
			"import": "./dist/es6/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/es6/index.js"
		}
	},
	"bugs": {
		"url": "https://github.com/vincehi/pino-email-transport/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"pino": "^7.0.0 || ^8.0.0 || ^9.0.0"
	},
	"dependencies": {
		"nodemailer": "^6.9.16",
		"pino-abstract-transport": "^1.0.0",
		"pino-pretty": "^11.3.0"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build:cjs": "tsc",
		"build:es6": "tsc -p tsconfig.es6.json",
		"build": "npm run clean && npm run build:cjs && npm run build:es6",
		"prepublishOnly": "npm run build",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"files": ["dist"],
	"devDependencies": {
		"@types/events": "^3.0.3",
		"@types/node": "^24.10.1",
		"@types/nodemailer": "^7.0.4",
		"@vitest/coverage-v8": "^4.0.13",
		"pino": "^10.1.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.13"
	},
	"private": false
}
